<% content_for :title, "Teams" %>

<h1>Teams</h1>

<div id="teams">
  <% @teams.each do |team| %>
    <%= render team %>
    <p>
      <%= link_to "Show this team", gaming_group_team_url(@gaming_group, team) %>
    </p>
  <% end %>
</div>

<%= link_to "New team", new_gaming_group_team_path(@gaming_group) %>